{
    "name": "yapro/doctrine-converter",
    "version": "1.0",
    "require": {
        "php": "7.4.*",
        "doctrine/orm": "*",
        "pmill/doctrine-array-hydrator": "^0.1.7",
        "yapro/helpers": "*"
    },
    "autoload": {
        "psr-4": {
            "YaPro\\DoctrineConverter\\": "src/"
        }
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "phpunit/phpunit": "*",
        "symfony/cache": "*",
        "symfony/serializer": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "YaPro\\DoctrineConverter\\Tests\\": "tests/"
        }
    }
}
